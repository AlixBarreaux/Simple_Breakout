[application]

config/name="Simple Breakout"
run/main_scene="res://Scenes/Main.tscn"
config/icon="res://Miscellaneous/icon.png"
config/version=1

[autoload]

Events="*res://Singletons/events.gd"
Settings="*res://Singletons/settings.gd"
Global="*res://Singletons/global.gd"

[display]

window/size/width=1980
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

version=0

[input]

move_left={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":81,"unicode":0,"echo":false,"script":null)
 ]
}
move_right={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":68,"unicode":0,"echo":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="Paddles"
2d_physics/layer_2="Balls"
2d_physics/layer_3="Terrain"
2d_physics/layer_4="BricksBallDetector"

[locale]

translations=PoolStringArray( "res://Translations/fr/Menus/menus_fr.po", "res://Translations/de/Menus/menus_de.po" )
locale_filter=[ 0, [  ] ]

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://Resources/Environments/default_env.tres"
